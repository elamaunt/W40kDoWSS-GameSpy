<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SteamSpy.Resources"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- Page names -->
    <system:String x:Key="MainPage">Главная</system:String>
    <system:String x:Key="ChatPage">Чат</system:String>


    <!-- Main Page -->
    <system:String x:Key="LaunchGame">Запустить игру</system:String>
    <system:String x:Key="FoundWarnings">Мы нашли незначительные неисправности с Вашей игрой. </system:String>
